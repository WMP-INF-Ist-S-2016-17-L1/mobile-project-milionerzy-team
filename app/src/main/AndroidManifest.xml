<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ur.edu.pl.millionaire">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Milionerzy"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ur.edu.pl.millionaire.ScoreActivity"
            android:label="@string/title_activity_scores" >
        </activity>
        <activity
            android:name="ur.edu.pl.millionaire.PlayActivity"
            android:label="@string/title_activity_play" >
        </activity>
        <activity
            android:name="ur.edu.pl.millionaire.HighscoreActivity"
            android:label="HighscoreActivity" >
        </activity>
    </application>

</manifest>